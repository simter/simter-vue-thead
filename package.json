{
  "name": "simter-vue-thead",
  "version": "0.4.2",
  "description": "Simter thead component",
  "keywords": [
    "simter",
    "vue",
    "thead",
    "rollup",
    "parcel"
  ],
  "main": "dist/simter-vue-thead.js",
  "module": "dist/simter-vue-thead.esm.js",
  "repository": "https://github.com/simter/simter-vue-thead.git",
  "author": "RJ.Hwang <rongjihuang@gmail.com>",
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs",
    "debug",
    "rollup.config.js"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel debug/index.html -d temp",
    "test": "ava",
    "clean": "rm -rf dist/* temp/*",
    "build": "npm run clean && npm test && rollup -c && npm run min-js",
    "min-js": "uglifyjs --comments /^!/ dist/simter-vue-thead.js -o dist/simter-vue-thead.min.js --source-map=url",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "ava": "~0.25.0",
    "browser-env": "~3.2.5",
    "parcel-bundler": "~1.10.2",
    "require-extension-hooks": "~0.3.2",
    "require-extension-hooks-babel": "~0.1.1",
    "require-extension-hooks-vue": "~1.1.0",
    "rollup": "^1.15.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-vue": "^5.0.0",
    "simter-vue-colgroup": "~0.3.0",
    "stylus": "~0.54.5",
    "uglify-es": "~3.3.9",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1",
    "vue-template-compiler": "~2.5.17"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup.js"
    ]
  }
}
